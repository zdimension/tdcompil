const string global = "test";

int main()
{
  static const int x = 10;
  const int i, j=7, k=8;             // i est initialisé à 0;
  
  print ((x+2==12) ? "SUCCESS\n": "FAILURE\n");
  print ((global == "test") ? "SUCCESS\n": "FAILURE\n");
  print ((global[0] == "t") ? "SUCCESS\n": "FAILURE\n");

  print((i+j+k == 15) ? "SUCCESS\n": "FAILURE\n");
  return 0;
}
