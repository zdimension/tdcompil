int main() {
    int x = 3;
    float y = 3.1;

    print("Value of x: ", x, "\n");
    print("Value of -x: ", -x, "\n");
    print("-3 + 10 = ", -3 + 10, "\n");

    print("Value of y: ", y, "\n");
    print("Value of -y: ", -y, "\n");
    print("-3.1 + 10 = ", -3.1 + 10, "\n");

    print((-3.1 == -y) ? "SUCCESS\n": "FAILURE\n");
}

