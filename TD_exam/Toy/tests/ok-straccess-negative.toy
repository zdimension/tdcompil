int main() {
  string s1 = "Hello";
  string s2 = "World";

  print((s1[-1] == "o")? "SUCCESS\n": "FAILURE\n");
  print((s1[-2] == "l")? "SUCCESS\n": "FAILURE\n");
  print((s1[-3] == "l")? "SUCCESS\n": "FAILURE\n");

  print((s2[-1] == "d")? "SUCCESS\n": "FAILURE\n");
  print((s2[-2] == "l")? "SUCCESS\n": "FAILURE\n");
  print((s2[-3] == "r")? "SUCCESS\n": "FAILURE\n");

  print((s1[-1] == s2[-4])? "SUCCESS\n": "FAILURE\n");
  print("SUCCESS\n");
  return 0;
}

