int main()
{
  int x = 1_000_000;
  int y = 1000000;

  print((x == y) ? "SUCCESS\n": "FAILURE\n");
  print((y+1 == 1_000_001)? "SUCCESS\n": "FAILURE\n");

  int sum = 0;
  for (int i = 1; i < 1_0_0; i++)
    sum = sum + i;
  print("sum = ", sum, "\n");
  print(sum == 4_950 ? "SUCCESS\n": "FAILURE\n");

  return 0;
}
