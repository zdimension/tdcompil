int main() {
    string ch1 = "abcd";
    string ch2 = "xyz";

    print(ch1, " < ",  ch2, " => ", ch1 <  ch2, "\n");
    print(ch1, " <= ", ch2, " => ", ch1 <= ch2, "\n");
    print(ch1, " > ",  ch2, " => ", ch1 >  ch2, "\n");
    print(ch1, " >= ", ch2, " => ", ch1 >= ch2, "\n");
    print(ch1, " == ", ch2, " => ", ch1 == ch2, "\n");
    print(ch1, " != ", ch2, " => ", ch1 != ch2, "\n");
    print("----------------\n");
    print(ch1, " < ",  ch1, " => ", ch1 <  ch1, "\n");
    print(ch1, " <= ", ch1, " => ", ch1 <= ch1, "\n");
    print(ch1, " > ",  ch1, " => ", ch1 >  ch1, "\n");
    print(ch1, " >= ", ch1, " => ", ch1 >= ch1, "\n");
    print(ch1, " == ", ch1, " => ", ch1 == ch1, "\n");
    print(ch1, " != ", ch1, " => ", ch1 != ch1, "\n");

    print(("abcd" < "z")? "SUCCESS\n": "FAILURE\n");
    print(("abc" < "abcd")? "SUCESSS\n": "FAILURE\n");
    return 0;
}


