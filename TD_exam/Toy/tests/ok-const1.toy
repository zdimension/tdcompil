int main()
{
  const int x = 10;
  int y = 8;

  print ((y < x) ? "SUCCESS\n": "FAILURE\n");
  y = y + 1;
  print ((y < x) ? "SUCCESS\n": "FAILURE\n");
  y = y + 1;
  print ((y == x) ? "SUCCESS\n": "FAILURE\n");
  return 0;
}
