{
	type fibo_iter = struct { a: u16; b: u16; };

	impl fibo_iter
	{
		func init(iter: fibo_iter*): void
		{
			(*iter).a = 0;
			(*iter).b = 1;
		}

		func hasNext(iter: fibo_iter*): bool
		{
			return true;
		}

		func next(iter: fibo_iter*): u16
		{
			var a = (*iter).a;
			var b = (*iter).b;
			(*iter).a = b;
			(*iter).b = a + b;
			return a;
		}
	}

	var iter: fibo_iter;
	iter.init();

	while (iter.hasNext())
	{
		var val = iter.next();
		if (val > 10)
			break;
		print val;
	}
}