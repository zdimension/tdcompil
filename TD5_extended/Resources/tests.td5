{
	type range_iter = struct { from: u16; to: u16; current: u16; };

	impl range_iter
	{
		func `(`(from: u16, to: u16): range_iter
		{
			return range_iter { from: from, to: to, current: from };
		}

		func hasNext(iter: range_iter*): bool
		{
			return (*iter).current < (*iter).to;
		}

		func next(iter: range_iter*): u16
		{
			return (*iter).current++;
		}
	}

	var x = range_iter(1, 10);

	for (var n in x)
	{
		print n;
	}
}