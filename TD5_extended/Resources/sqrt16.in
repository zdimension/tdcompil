{
	# requires INT_WIDTH = 16
	func isqrt(x)
	{
		var rem = 0, root = 0, i;

		for (i = 0; i < 8; i++)
		{
			root = root << 1;
			rem = (rem << 2) + (x >> 14);
			x = x << 2;
			root++;
			if (root <= rem)
			{
				rem -= root;
				root++;
			}
			else
			{
				root--;
			}
		}

		return root >> 1;
	}

	print isqrt(32768);
}