{
	var ptr, cur, eq[] = "2 3 4 * +.", stack[10], sp = stack;
	for (ptr = eq; {cur = *ptr; cur != '.'}; ptr++)
	{
		if (cur == ' ')
		{
			sp++;
		}
		else if (cur == '+')
		{
			b = *(--sp);
			a = *(--sp);
			*sp = a + b;
		}
		else if (cur == '*')
		{
			b = *(--sp);
			a = *(--sp);
			*sp = a * b;
		}
		else if (cur >= '0' && cur <= '9')
		{
			*sp = (*sp * 10) + (cur - '0');
		}
	}
	print stack[0];
