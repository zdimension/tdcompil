int main() {
    int x = 0;
    int y;

    y = x++; print("(after y = x++) ", "x=", x, " y=", y, "\n");
    y = ++x; print("(after y = ++x) ", "x=", x, " y=", y, "\n");

    print("-++y  = ", -++y , "\n");
    print("- --y = ", - --y , "\n");
    print("-y++  = ", -y++ , "\n");
    print("-y--  = ", -y-- , "\n");

    print((x == 2 and y == 2) ? "SUCCESS\n": "FAILURE\n");
    return 0;
}
