int main() {
  print("Test with numbers\n");
  print("Ox12 = ", 0x12, "\n");
  print("012  = ", 012,  "\n");
  print("12   = ", 12,   "\n");
  print("+12  = ", +12,  "\n");
  print("-12  = ", -12,  "\n");
  print("End of test: ", (0x12 == 022) ? "SUCCESS\n": "FAILURE\n");
  return 0;
}
